# .env.example — заполните локально .env на машине, НЕ коммитьте реальные ключи
PORT=3001
PUBLIC_URL=https://onetapday-mvp.onrender.com
STRIPE_SECRET_KEY=REPLACE_WITH_YOUR_STRIPE_SECRET_KEY
STRIPE_WEBHOOK_SECRET=REPLACE_WITH_YOUR_WEBHOOK_SECRET
STRIPE_PRICE_ID=REPLACE_WITH_YOUR_PRICE_ID_MONTHLY
# Optional plans (enable buttons if set):
STRIPE_PRICE_ID_6M=
STRIPE_PRICE_ID_YEARLY=
STRIPE_PUBLISHABLE_KEY=REPLACE_WITH_YOUR_PUBLISHABLE_KEY
NODE_ENV=production

# OpenAI / AI-консультант
OPENAI_API_KEY=
OTD_AI_ENABLED=1
OTD_AI_MODEL=gpt-4o-mini
OTD_AI_MAX_OUTPUT_TOKENS=600
OTD_AI_REQS_PER_MIN=10
OTD_AI_REQS_PER_DAY=300
OTD_AI_TOKENS_PER_DAY=200000
