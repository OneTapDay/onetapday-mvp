<!-- ЗАКРЫТЬ СМЕНУ -->
<div style="margin-top:12px; text-align:right;">
<button class="btn" data-i18n="btn_close_shift" id="cashClose" onclick="
          (function(){
            if (typeof kasaBalance !== 'function' || typeof addKasa !== 'function') {
              console.error('kasaBalance/addKasa missing');
              return;
            }

            var current = kasaBalance().toFixed(2);
            var a = prompt('Итог в кассе (PLN):', current);
            if (a === null) return;

            var raw = String(a).replace(',', '.');
            var v = parseFloat(raw);
            if (!v || !isFinite(v)) {
              alert('Сумма некорректна');
              return;
            }

            addKasa('zamknięcie', v, 'close', 'manual');

            if (typeof showKasaTodayStats === 'function') {
              showKasaTodayStats();
            }
          })();
        ">
  Zamknij zmianę
</button>
