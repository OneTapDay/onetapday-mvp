<div class="section" id="kasa">
<div class="muted small" data-i18n="cash_intro" style="margin:6px 0 10px 0">
      Tu zapisujesz ruchy gotówki: przyjęcia i wydania. One trafią do podsumowań i księgi.
    </div>
<div class="card">
<!-- Заголовок кассы -->
<div data-i18n="cash_title" style="font-weight:700; font-size:18px;">
        Kasa — szybkie operacje
      </div>
<div class="muted small" data-i18n="cash_note" style="margin-top:4px">
        Głosowa kasa + szybkie przyciski
      </div>
<!-- БЫСТРЫЕ ОПЕРАЦИИ (сетка 2x2) -->
<div style="margin-top:16px">
<div class="muted small" style="margin-bottom:6px">
          Быстрые операции
        </div>
<div class="kasa-quick" id="kasaQuickHolder" style="display:grid;grid-template-columns:repeat(auto-fit,minmax(180px,1fr));gap:8px;">
<!-- Кнопки пресетов создаются через JS, контейнер просто стал сеткой -->
</div>
</div>
<!-- РАСХОД / МИКРОФОН / ДОХОД -->
<div style="margin-top:16px; display:flex; justify-content:center; gap:8px; align-items:center; flex-wrap:wrap;">
<!-- Расход -->
<button class="btn secondary" data-i18n="btn_add_out" id="addOut" onclick="
          (function(){
            var amtEl  = document.getElementById('quickAmt');
            var noteEl = document.getElementById('quickNote');
            var catSel = document.getElementById('quickCashCat');
            if (!amtEl) return;

            var raw = String(amtEl.value || '').replace(',', '.');
            var val = parseFloat(raw);
            if (!val || !isFinite(val)) {
              alert('Сначала введи сумму');
              return;
            }

            var note = (noteEl &amp;&amp; noteEl.value || '').trim();
            var cat  = (catSel &amp;&amp; catSel.value) || '';

            if (typeof addKasa === 'function') {
              addKasa('wydanie', val, note, 'manual', cat);
              amtEl.value = '';
              if (noteEl) noteEl.value = '';
            } else {
              console.error('addKasa not found');
            }
          })();
        " style="flex:1; min-width:140px; max-width:200px;">
  − <span data-i18n="cash_expense_label">Wydatek</span>
</button>
<!-- Микрофон -->
<button class="mic" id="micBtn" style="width:44px;height:44px;border-radius:999px;display:flex;align-items:center;justify-content:center;" title="Voice">
          🎤
        </button>
<!-- Доход -->
<button class="btn secondary" data-i18n="btn_add_in" id="addIn" onclick="
          (function(){
            var amtEl  = document.getElementById('quickAmt');
            var noteEl = document.getElementById('quickNote');
            var catSel = document.getElementById('quickCashCat');
            if (!amtEl) return;

            var raw = String(amtEl.value || '').replace(',', '.');
            var val = parseFloat(raw);
            if (!val || !isFinite(val)) {
              alert('Сначала введи сумму');
              return;
            }

            var note = (noteEl &amp;&amp; noteEl.value || '').trim();
            var cat  = (catSel &amp;&amp; catSel.value) || '';

            if (typeof addKasa === 'function') {
              addKasa('przyjęcie', val, note, 'manual', cat);
              amtEl.value = '';
              if (noteEl) noteEl.value = '';
            } else {
              console.error('addKasa not found');
            }
          })();
        " style="flex:1; min-width:140px; max-width:200px;">
  + <span data-i18n="cash_income_label">Przyjęcie</span>
</button>
</div>
<!-- СУММА / КОММЕНТ / КАТЕГОРИЯ (стеком, как на макете) -->
